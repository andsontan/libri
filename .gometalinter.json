{
  "Enable": [
    "aligncheck",
    "deadcode",
    "dupl",
    "errcheck",
    "gas",
    "goconst",
    "gocyclo",
    "golint",
    "gosimple",
    "lll",
    "misspell",
    "ineffassign",
    "interfacer",
    "staticcheck",
    "structcheck",
    "unconvert",
    "unused",
    "varcheck",
    "vet",
    "vetshadow"
  ],
  "VendoredLinters": true,
  "Concurrency": 8,
  "LineLength": 100,
  "Deadline": 300000000000,
  "Format": "({{.Linter}}): {{.Severity}}: {{.Path}}:{{.Line}}:{{if .Col}}{{.Col}}{{end}}: {{.Message}}",
  "Linters": {
    "golint": "golint -min_confidence {min_confidence} $(find {path} -name *.go ! -name *.pb.go):PATH:LINE:COL:MESSAGE",
    "dupl": "dupl -plumbing -threshold {duplthreshold} $(find {path} -name *.go ! -name *.pb.go):^(?P<path>.*?\\.go):(?P<line>\\d+)-\\d+:\\s*(?P<message>.*)$",
    "lll": "lll -g -l {maxlinelength} $(find {path} -name *.go ! -name *.pb.go):PATH:LINE:MESSAGE",
    "vet": "go tool vet $(find {path} -name *.go ! -name */vendor/* ):^(?:vet:.*?\\.go:\\s+(?P<path>.*?\\.go):(?P<line>\\d+):(?P<col>\\d+):\\s*(?P<message>.*))|(?:(?P<path>.*?\\.go):(?P<line>\\d+):\\s*(?P<message>.*))$",
    "vetshadow": "go tool vet --shadow $(find {path} -name *.go ! -name */vendor/* ):^(?:vet:.*?\\.go:\\s+(?P<path>.*?\\.go):(?P<line>\\d+):(?P<col>\\d+):\\s*(?P<message>.*))|(?:(?P<path>.*?\\.go):(?P<line>\\d+):\\s*(?P<message>.*))$"
  }
}